<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>WinExe</OutputType>
		<TargetFramework>net7.0-windows10.0.17763.0</TargetFramework>
		<UseWindowsForms>true</UseWindowsForms>
		<StartupObject>PasswordManager.Program</StartupObject>
		<ApplicationIcon>IconManager.ico</ApplicationIcon>
		<PackageId>InfoAssistancePasswordManager</PackageId>
		<Authors>InfoAssistance</Authors>
		<Description>Gestionnaire de mot de passe </Description>
		<Copyright>2023</Copyright>
		<PackageIcon>icons8-mot-de-passe-100.png</PackageIcon>
		<NeutralLanguage>fr</NeutralLanguage>
	</PropertyGroup>

	<ItemGroup>
		<None Remove=".gitattributes" />
		<None Remove=".gitignore" />
		<None Remove="README.md" />
	</ItemGroup>

	<ItemGroup>
	  <Content Include="IconManager.ico" />
	</ItemGroup>

	<ItemGroup>
		<Folder Include="Properties\" />
	</ItemGroup>

	<ItemGroup>
	  <None Include="..\..\icons8-mot-de-passe-100.png">
	    <Pack>True</Pack>
	    <PackagePath>\</PackagePath>
	  </None>
	</ItemGroup>

</Project>